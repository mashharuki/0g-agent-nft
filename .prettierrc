{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false,
        "printWidth": 100
      }
    }
  ]
}
